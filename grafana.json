      "targets": [{ "expr": "count(ifOperStatus{job=\"cisco_snmp\", instance=\"$instance\"} == 2) or vector(0)", "instant": true, "refId": "A" }],
      "title": "Ports Down",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
